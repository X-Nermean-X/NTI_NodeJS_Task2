{{>header}}
<br>
    <h1 style="text-align: center;">{{h1}}</h1>
    <div class="features">
        <div class="container">
            <div class="row">
                {{#each data}}
                    <div class="col-lg-4 col-md-6" style="padding: 10px;">
                        <div class="card">
                            <img src="{{image_url}}" class="card-img-top" alt="...">
                            <div class="card-body">
                                <h4 class="card-title">{{title}}</h4>
                                <p class="card-text">{{description}}</p>
                            </div>
                        </div> 
                    </div>    
                {{/each}}
            </div>
        </div>
    </div>
<br>
{{>footer}}
